MODULE PBO OUTPUT.

  LOOP AT SCREEN.

    IF SCREEN-GROUP1 = 'C1' AND
        GV_DISPLAY = ABAP_TRUE.
      SCREEN-INPUT = 0.
       MODIFY SCREEN.
    ENDIF.

  ENDLOOP.

ENDMODULE.
